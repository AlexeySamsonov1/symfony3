<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        {#stylesheets#}
        <link rel="stylesheet" href="{{ asset('css/libraries/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/drag-and-drop.css') }}">
        {#javascripts#}
        <script src="{{ asset('js/libraries/jquery-3.1.1.min.js') }}"></script>
        <script src="{{ asset('js/libraries/bootstrap.min.js') }}"></script>


    </head>
    <body>
        <div class="container">
            {% block body %}{% endblock %}
            {% block javascripts %}
                <script src="{{ asset('js/drag-and-drop.js') }}"></script>
            {% endblock %}
        </div>
    </body>
</html>
